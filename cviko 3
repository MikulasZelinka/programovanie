def soucet_a_nasobek(x, y):
    soucet = x + y
    nasobek = x * y
    return soucet, nasobek

vysledek = soucet_a_nasobek(2, 3)

vysl_soucet, vysl_nasobek = vysledek
vysl_soucet = vysledek[0]
vysl_nasobek = vysledek[1]

print(vysledek)

def mocniny(x):
    return x * x, x * x * x

print(mocniny(3))
druha, treti = mocniny(3)



